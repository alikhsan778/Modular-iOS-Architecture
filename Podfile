# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

workspace 'Modular iOS Architecture'

target 'Modular iOS Architecture' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Modular iOS Architecture
  pod 'Alamofire'
  pod 'SwiftyJSON'

end

target 'WatchlistKit' do
  project 'WatchlistKit/WatchlistKit'
  
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for WatchlistKit
  pod 'SwiftyJSON'

end

target 'NetworkingKit' do
  project 'NetworkingKit/NetworkingKit'
  
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for NetworkingKit
  pod 'Alamofire'
  pod 'SwiftyJSON'

end
